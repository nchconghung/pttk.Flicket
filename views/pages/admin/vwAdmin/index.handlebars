<!-- partial -->
<div class="main-panel">
    <div class="content-wrapper">
        <div class="card">
            <div class="card-body">
                <div style="display: flex;justify-content: space-between;align-items: baseline;">
                    <h3 class="card-title">Danh sách Admin</h3>
                    <button class="btn btn-success">Thêm admin</button>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                        <div id="order-listing_wrapper" class="dataTables_wrapper container-fluid dt-bootstrap4 no-footer">
                            <div class="row">
                            <div class="col-sm-12">
                                <table id="order-listing" class="table dataTable no-footer" role="grid" aria-describedby="order-listing_info">
                            <thead>
                            <tr role="row">
                                <th class="sorting_asc" tabindex="0" aria-controls="order-listing" rowspan="1" colspan="1" style="width: 63px;font-weight: bold;" aria-sort="ascending" >#</th>
                                <th  class="sorting" tabindex="0" aria-controls="order-listing" rowspan="1" colspan="1" style="width: 79px;font-weight: bold;" >Tài khoản</th>
                                <th class="sorting" tabindex="0" aria-controls="order-listing" rowspan="1" colspan="1" style="width: 100px;font-weight: bold;" >Lần đăng nhập cuối</th>
                                
                            </tr>
                            </thead>
                            <tbody>
                                {{#each list}}
                                    <tr role="row" >
                                        <td class="sorting_1">{{this.Id}}</td>           
                                        <td>{{this.TaiKhoan}}</td>
                                        <td>{{this.LanDangNhapCuoi}}</td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                        </div>
                    </div>
                    </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
